{
  "expo": {
    "name": "For_Us",
    "slug": "For_Us",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#e4d8c8"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.citizencomplaint.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#e4d8c8"
      },
      "package": "com.citizencomplaint.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to location to automatically detect your location for reports."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "This app needs access to photos to allow you to attach images to your reports.",
          "cameraPermission": "This app needs access to camera to allow you to take photos for your reports."
        }
      ]
    ],
    "scheme": "for_us"
    
  }
}
